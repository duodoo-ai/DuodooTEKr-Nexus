<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.ui.view" id="economic_contract_data_tree">
        <field name="name">经法合同数据模板.tree</field>
        <field name="model">economic.contract.data</field>
        <field name="arch" type="xml">
            <list string="经法合同数据模板" editable="top" default_order="id desc">
                <field name="name" optional="show"/>
                <field name="code" optional="show"/>
                <field name="contract_code" optional="show"/>
                <field name="contract_category" options="{'no_open': True, 'no_create': True}" optional="show"/>
                <field name="contract_organizer" options="{'no_open': True, 'no_create': True}" optional="show"/>
                <field name="contract_opposing" options="{'no_open': True, 'no_create': True}" optional="show"/>
                <field name="date_signing" optional="show"/>
                <field name="contract_amount" optional="show"/>
                <field name="contract_amount_exclude_tax" optional="show"/>
                <field name="company_id" optional="hide"/>
                <field name="id" optional="hide"/>
            </list>
        </field>
    </record>

    <record id="action_economic_contract_data" model="ir.actions.act_window">
        <field name="name">经法合同数据模板</field>
        <field name="res_model">economic.contract.data</field>
        <field name="view_mode">list</field>
        <field name="search_view_id" ref=""/>
        <field name="view_ids"
           eval="[(5, 0, 0),
                  (0, 0, {'view_mode': 'list', 'view_id': ref('economic_contract_data_tree')}),
                  ]"/>
    </record>
</odoo>