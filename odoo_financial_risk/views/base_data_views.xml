    <record model="ir.ui.view" id="base_contract_category_tree">
        <field name="name">合同分类及名称.tree</field>
        <field name="model">base.contract.category</field>
        <field name="arch" type="xml">
            <list string="合同分类及名称" editable="top">
                <field name="name" optional="show"/>
                <field name="code" optional="show"/>
                <field name="company_id" optional="hide"/>
                <field name="id" optional="hide"/>
            </list>
        </field>
    </record>

    <record id="action_base_contract_category" model="ir.actions.act_window">
        <field name="name">合同分类及名称</field>
        <field name="res_model">base.contract.category</field>
        <field name="view_mode">list</field>
        <field name="search_view_id" ref=""/>
        <field name="view_ids"
           eval="[(5, 0, 0),
                  (0, 0, {'view_mode': 'list', 'view_id': ref('base_contract_category_tree')}),
                  ]"/>
    </record>

